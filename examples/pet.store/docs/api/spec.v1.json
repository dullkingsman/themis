{
  "openapi": "3.0.0",
  "info": {
    "title": "Pets API",
    "version": "You can work with pets data."
  },
  "paths": {
    "/pet": {
      "get": {
        "responses": {
          "200": {
            "description": "The world at your finder tip.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string"
                      },
                      "name": {
                        "type": "string"
                      },
                      "age": {
                        "type": "number"
                      },
                      "type": {
                        "type": "string",
                        "enum": [
                          "HUSKY",
                          "BULLDOG",
                          "GERMAN SHEPHERD"
                        ]
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}